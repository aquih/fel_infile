<?xml version="1.0" encoding="utf-8"?>
<openerp>
    <data>

        <record model="ir.ui.view" id="invoice_form_fel_infile">
            <field name="name">account.invoice.form.gface_infile</field>
            <field name="model">account.invoice</field>
            <field name="inherit_id" ref="account.invoice_form"/>
            <field name="arch" type="xml">
                <data>
                    <field name="comment" position="before">
                        <group>
                            <field name="uuid_fel"/>
                            <field name="pdf_fel" widget="url"/>
                        </group>
                    </field>
                </data>
            </field>
        </record>

        <record model="ir.ui.view" id="journal_form_fel_infile">
            <field name="name">account.journal.form.gface_infile</field>
            <field name="model">account.journal</field>
            <field name="inherit_id" ref="account.view_account_journal_form"/>
            <field name="arch" type="xml">
                <data>
                    <field name="group_invoice_lines" position="after">
                        <field name="usuario_fel"/>
                        <field name="clave_fel"/>
                        <field name="token_firma_fel"/>
                        <field name="codigo_establecimiento_fel"/>
                        <!-- <field name="nombre_establecimiento_fel"/>
                        <field name="tipo_documento_fel"/>
                        <field name="serie_documento_fel"/>
                        <field name="serie_fel"/>
                        <field name="numero_resolucion_fel"/>
                        <field name="fecha_resolucion_fel"/>
                        <field name="rango_inicial_fel"/>
                        <field name="rango_final_fel"/>
                        <field name="numero_establecimiento_fel"/>
                        <field name="dispositivo_fel"/> -->
                    </field>
                </data>
            </field>
        </record>

    </data>
</openerp>
